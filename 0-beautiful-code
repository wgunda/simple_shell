if (message != NULL)
{
int i = 0;
while (message[i] != '\0')
{
putchar(message[i]);
i++;
}
putchar('\n');
}
}
/**
* main - Entry point
*
* Description: This is the main function that
* calls the print_message function.
*
* Return: Always 0 (Success)
*/
int main(void)
{
const char *message = "Hello, World!";
print_message(message);
return (0);
}

